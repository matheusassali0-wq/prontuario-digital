{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "verbatimModuleSyntax": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "baseUrl": ".",
"types": [
  "vite/client"
],
"paths": {
  "@contracts": [
    "../contracts/src/index.ts"
  ],
  "@contracts/*": [
    "../contracts/src/*"
  ]
},
},
"exclude": [
  "build",
  ".next",
  "coverage"
]
}